{{page-title "Wallet"}}

<div class="content">
  <div class="row">
    <div class="col-lg-12 col-md-12">
      {{wallets/wallet-list onRouteChange=this.setQueryParam}}
    </div>
  </div>
  <div class="row">
    <div class="col-lg-4 col-md-12">
      {{charts/holding-pie onRouteChange=this.setQueryParam}}
    </div>
    <div class="col-lg-4 col-md-12">
      {{coinbases/coinbase-history onRouteChange=this.setQueryParam}}
    </div>
     <div class="col-lg-4 col-md-12">
      {{coinbases/een-coinbase-history onRouteChange=this.setQueryParam}}
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      {{transactions/transaction-history transactions=@model.transactions pagination=@model.pagination}}
     </div>
  </div>
</div>
