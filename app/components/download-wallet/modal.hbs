<div {{did-insert this.setupEventListener}} class="modal fade" id="downloadThetaExtension" tabindex="-1" role="dialog" aria-labelledby="downloadThetaExtension" style="display: none;" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div id="wallet-connection-notif">
                <div class="relative-content alert alert-primary">
                    <span data-notify="message">{{t 'download_wallet.make_sure'}} <a class="loading-a" href="https://docs.thetatoken.org/docs/chrome-wallet-extension" target="_blank">{{t 'download_wallet.theta_wallet_extension'}}</a>.
                    <br/>{{t 'global.click'}}
                        <a class="loading-a" href="https://chrome.google.com/webstore/detail/theta-wallet/ckelpdlfgochnkdgikcgbimdcfgpkhgk" target="_blank">{{t 'global.here'}}</a>
                    {{t 'download_wallet.download'}}</span>
                </div>
            </div>
        </div>
    </div>
</div>